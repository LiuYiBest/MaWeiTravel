<view class="container"><view class="header bg-ff"></view><map id="map" scale="{{map.scale}}" show-location="{{map.showLocation}}" longitude="{{map.longitude}}" latitude="{{map.latitude}}" width="{{map.width}}" height="{{map.height}}" controls="{{map.controls}}" markers="{{map.markers}}" data-event-opts="{{[['regionchange',[['mapChange',['$event']]]]]}}" bindregionchange="__e"><cover-image class="icon-img" src="/static/icon_position.png"></cover-image></map><view class="footer bg-ff font-26"><scroll-view class="scroll" scroll-y="{{true}}" scroll-top="{{scrollTop}}"><view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['bindAddress',[index]]]]]}}" class="padding border-b position-r" bindtap="__e"><view class="row">{{item.title}}</view><view class="row color-99">{{item.address}}</view><block wx:if="{{checked===index}}"><icon class="icon_circle" type="success" color="#E74246" size="22"></icon></block></view></block></view></scroll-view></view></view>